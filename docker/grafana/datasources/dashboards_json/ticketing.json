{
  "uid": "ticketing-main",
  "title": "Ticketing Overview",
  "schemaVersion": 39,
  "version": 2,
  "panels": [
    {
      "type": "stat",
      "title": "Tickets Sold (total)",
      "targets": [{ "expr": "sum(tickets_sold_total)" }],
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 }
    },
    {
      "type": "stat",
      "title": "Purchase Conflicts",
      "targets": [{ "expr": "sum(purchase_conflict_total)" }],
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Purchase Latency (P95 & P99)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(purchase_latency_bucket[1m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(purchase_latency_bucket[1m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "gridPos": { "x": 0, "y": 6, "w": 16, "h": 8 }
    },
    {
      "type": "graph",
      "title": "HTTP Throughput (RPS)",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count[1m]))",
          "legendFormat": "req/s"
        }
      ],
      "gridPos": { "x": 0, "y": 14, "w": 16, "h": 8 }
    }
  ]
}
